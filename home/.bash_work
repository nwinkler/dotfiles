export BASH_IT_HTTP_PROXY_ALT=http://localhost:8999
export BASH_IT_HTTPS_PROXY_ALT=$BASH_IT_HTTP_PROXY_ALT

export BASH_IT_HTTP_PROXY=http://10.244.145.252:8080
export BASH_IT_HTTPS_PROXY=$BASH_IT_HTTP_PROXY

# 192.168.59.103 and 192.168.99.100 are for Docker
export BASH_IT_NO_PROXY=127.0.0.1,localhost,192.168.59.103,192.168.99.100,services.sungard.com,repo.devcloud.sungard.com

