#!/bin/bash

export MAVEN_OPTS=-Xmx512m

pathmunge ~/bin:/usr/local/sbin

export HOMEBREW_CASK_OPTS="--appdir=/Applications"

export GIT_HOSTING='ssh://git@ssh.github.com:443/'

export BASH_IT_DOCKER_MACHINE="default"

# Silence the macOS zsh warning, see here:
# https://www.reddit.com/r/osx/comments/cmm2on/catalina_suppress_zsh_warning/
export BASH_SILENCE_DEPRECATION_WARNING=1
